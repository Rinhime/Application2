<%= render 'users/user', user: @user %>
<%= render 'books/book', book: @book %>
<main>
  <h1>投稿一覧</h1>
　<% @books.each do |book| %>
  <div>
    <p><%= image_tag @user.get_profile_image(100,100) %></p>
    <%= link_to book_path(book.id) do %>
      <p><%= book.title %></p>
    <% end %>
    <p><%= book.body %></p>
  </div>
<% end %>
</main>