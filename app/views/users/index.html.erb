<h1>Usersユーザー一覧</h1>
<p>image</p>
<p>name</p>

<% @users.each do |users| %>
  <div>
    <%= image_tag users.get_image(100,100) %>
    
    <p>name：<%= users.name %></p>
  </div>
<% end %>
<sidebar>
<h1>New book</h1>
    <%= form_with model: @book do |f| %>
      <h4>Title</h4>
      <%= f.text_field :title %>
      <h4>Opinion</h4>
      <%= f.text_area :body %>
      <%= f.submit 'Create Book' %>
    <% end %>
</sidebar>