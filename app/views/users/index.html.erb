<h1>Usersユーザー一覧</h1>
<p>image</p>
<p>name</p>

<% @users.each do |users| %>
  <div>
    <%= image_tag users.get_profile_image(100,100) %>
    <p>name：<%= users.name %></p>
    <%= link_to "Show", user_path(users.id) %>
  </div>
<% end %>

<%= render 'users/user', user: @user %>

<%= render 'books/book', book: @book %>